{
  "name": "vue-grid-table",
  "version": "0.0.2",
  "description": "A Vue.js Table Component with resizable columns and a sticky table head",
  "author": {
    "name": "3zbumban",
    "email": "free4allcoding@gmail.com"
  },
  "repository": "https://github.com/3zbumban/vue-grid-table",
  "bugs": "https://github.com/3zbumban/vue-grid-table/issues",
  "keywords": ["vue", "vuejs", "css-grid", "table", "sticky-header", "resizable", "grid"],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue-grid-table ./src/components/VTable.vue",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/vue-grid-table.umd.js",
  "module": "./dist/vue-grid-table.esm.js",
  "unpkg": "./dist/vue-grid-table.min.js",
  "browser": {
    "./sfc": "src/components/VTable.vue"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.1",
    "@vue/cli-plugin-eslint": "~4.3.1",
    "@vue/cli-service": "~4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  }
}
